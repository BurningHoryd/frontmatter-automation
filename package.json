{
	"name": "frontmatter-generator",
	"version": "1.0.0",
	"description": "Generates and updates frontmatter (title, summary, and tags) for notes in Obsidian.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"frontmatter",
		"tags",
		"metadata",
		"note-taking"
	],
	"author": "BurningHoryd",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/BurningHoryd/frontmatter-generator.git"
	},
	"homepage": "https://github.com/BurningHoryd/frontmatter-generator#readme",
	"bugs": {
    "url": "https://github.com/BurningHoryd/frontmatter-generator/issues"
	},
	"devDependencies": {
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"js-yaml": "^4.1.0"
	}
}
